    "def timeset (H, M):\n",
    "    total = H*60 + M - 45\n",
    "    timeN = divmod(total,60)\n",
    "    return print(f'{timeN[0]} {timeN[1]}')\n",
    "\n",
    "if (H == 0) & (M < 45):\n",
    "    H = H + 24\n",
    "    timeset(H, M)\n",
    "else:\n",
    "    timeset(H, M)"
